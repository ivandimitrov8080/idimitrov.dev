    "eslint-config-prettier": "latest",
    "eslint-plugin-svelte": "latest",
    "globals": "latest",
    "prettier": "latest",
    "prettier-plugin-svelte": "latest",
    "svelte": "latest",
    "svelte-check": "latest",
    "svelte-preprocess": "latest",
    "typescript": "latest",
    "typescript-eslint": "latest",
    "vite": "latest",
    "vitest": "latest"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
